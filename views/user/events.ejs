      .card img {
        max-width: 25%;
        margin: auto;
        padding: 0.5em;
        border-radius: 0.7em;
      }
      a{
    text-decoration: none;
    color: rgb(39,81,163);
}
.list-link p{
    line-height: 13px;
}
