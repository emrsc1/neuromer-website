  <body style="font-family: 'Poppins', sans-serif; text-decoration: none">
    <!--Navbar-->
    <%- include('../partials/nav') %>

    <div class="left-part">
      <div class="contain">
        <div class="row">
          <div class="col-md-8">
            <div class="card">
              <img
